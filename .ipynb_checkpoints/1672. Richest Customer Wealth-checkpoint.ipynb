    "def rich(acc):\n",
    "    temp = 0\n",
    "    for ele in acc:\n",
    "        if sum(ele)>temp:\n",
    "            temp = sum(ele)\n",
    "    return temp\n",
    "    \n",
    "acc = [[2,8,7],[7,1,3],[1,9,5]]\n",
    "print(rich(acc))"
   ]
